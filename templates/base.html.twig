{% extends 'base_inspinia.html.twig' %}



{% block body %}
 

 <div id="wrapper">
      {{ include ('Default/includes/flashMessages.html.twig')}} 
 
       <!-- Left side column. contains the logo and sidebar -->
      {{render(controller('App\\Controller\\AdminController::includeAsideLeft'))}}

      

       <!-- /.sidebar -->
            

      <!-- Content Wrapper. Contains page content -->
      <!--<div class="content-wrapper">-->
      <!--white-bg dashbard-1-->
    <div id="page-wrapper" class="gray-bg dashbard-1">


      <!-- ENCABEZADO -->
      {{render(controller('App\\Controller\\AdminController::includeHead')) }}
      <!-- FIN ENCABEZADO -->
    
      <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-lg-12 col-md-12 col-xs-12">
          <h2><strong>{% block headercontent %}{% endblock %}</strong></h2>              
         </div>   
        </div>    
      <br>

    
    <div class="wrapper-content white-bg">   
      <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
          <br>
        {% block content %}{% endblock %}
        </div>
      </div> 
      </div><!-- /.content-wrapper -->
      
      <br>
       <div class="footer">
          <div class="float-left">
             <strong>Copyright &copy; {{ "now "   | date('Y')}} - {{ "now"   |date_modify("+1 year")| date('Y')}} <a href="{{path('app_homepage')}}">{% trans %}www.venetis.com.ve{% endtrans %}</a>.</strong> 
          </div>

          <div>
             {% trans %}Todos los derechos reservados.{% endtrans %}
          </div>
          {% block footer %}{% endblock %}
        </div>

        
  
  </div><!-- ./page-wrapper -->    
  </div><!-- ./wrapper -->


{% endblock %}